@page
@model BoathouseDotNet.Pages.CreateAccountModel
@{
}
@if (Model.Success)
{
	<div class="">
		<h1 class="display-4">Login successfull</h1>
		<p>You can now continue to the <a asp-page="Account">Account Page</a>.</p>
	</div>
}
else
{
	<div class="">
		<h1 class="display-4">Create Account</h1>
		<p>This page simulates an application login.</p>
		<form method="post">
			<button>Login</button>
		</form>
	</div>
}


